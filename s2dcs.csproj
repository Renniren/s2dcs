<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <StartupObject>S2GameEntry</StartupObject>
    <RootNamespace>s2d</RootNamespace>
    <AssemblyName>s2d</AssemblyName>
    <Nullable>enable</Nullable>
    <ApplicationIcon />
    <Win32Resource />
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath></OutputPath>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Prefer32Bit>true</Prefer32Bit>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Box2D.NetStandard" Version="1.0.4" />
    <PackageReference Include="DotNetZip" Version="1.16.0" />
    <PackageReference Include="ImGui.NET" Version="1.89.4" />
    <PackageReference Include="Saffron.Util.ImGuiSFML" Version="1.3.0" />
    <PackageReference Include="sfml-graphics" Version="2.4.2" />
    <PackageReference Include="SFML.Net" Version="2.5.0" />
    <PackageReference Include="System.Text.Json" Version="7.0.1" />
  </ItemGroup>

</Project>
